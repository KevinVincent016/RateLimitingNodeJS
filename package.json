{
    "name": "rate-limiting-demo",
    "version": "1.0.0",
    "description": "Example of Rate Limiting Pattern",
    "main": "src/app.js",
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "start": "node src/app.js",
        "dev": "nodemon src/app.js",
